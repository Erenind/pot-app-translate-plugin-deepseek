{
  "id": "plugin.com.Erenind.deepseek",
  "plugin_type": "translate",
  "icon": "deepseek.svg",
  "display": "deepseek",
  "homepage": "https://github.com/erenind/pot-app-translate-plugin-deepseek",
  "needs": [
    {
      "key": "model",
      "display": "模型选择",
      "type": "select",
      "options": {
        "deepseek-chat": "deepseek-chat",
        "deepseek-reasoner": "deepseek-reasoner"
      }
    },
    {
      "key": "apiKey",
      "display": "API密钥",
      "type": "input"
    },
    {
      "key": "usages",
      "display": "单词信息",
      "type": "select",
      "options": {
        "full": "详细(发音/释义/用法)",
        "basic": "简略(用法/例句)",
        "none": "仅翻译"
      }
    },
    {
      "key": "hyphenMode",
      "display": "连字符短语模式",
      "type": "select",
      "options": {
        "on": "启用(-标识/连接短语,如-make-up)",
        "off": "禁用"
      }
    },
    {
      "key": "phraseUsages",
      "display": "短语信息",
      "type": "select",
      "options": {
        "full": "详细(发音/每部分释义/用法)",
        "basic": "简略(用法/例句/相关短语)",
        "none": "仅翻译"
      }
    },
    {
      "key": "custom_mode",
      "display": "自定义指令模式",
      "type": "select",
      "options": {
        "on": "启用(可使用///指令>内容格式)",
        "off": "禁用"
      },
      "default": "on"
    },
    {
      "key": "destription",
      "display": "以下部分的默认值: (下面未选择时显示的并不准确，但选择后正常，参数的具体作用见下，注意非必要不用手动选择) temperature=0.1, top_p=0.99, frequency_penalty=0, presence_penalty=0, max_tokens=2000",
      "type": "destription"
    },
    {
      "key": "temperature",
      "display": "输出随机性(temperature)",
      "type": "select",
      "options": {
        "0.1": "0.1 - 精准模式(最稳定)",
        "0.5": "0.5 - 平衡模式",
        "0.8": "0.8 - 创意模式",
        "1.2": "1.2 - 高随机性",
        "1.5": "1.5 - 实验性输出"
      }
    },
    {
      "key": "top_p",
      "display": "输出多样性(top_p)",
      "type": "select",
      "options": {
        "0.9": "0.9 - 精准筛选",
        "0.95": "0.95 - 适度多样",
        "0.99": "0.99 - 广泛采样",
        "1.0": "1.0 - 完全开放"
      }
    },
    {
      "key": "frequency_penalty",
      "display": "重复控制(frequency_penalty)",
      "type": "select",
      "options": {
        "-1.0": "-1.0 - 允许更多重复",
        "0": "0 - 默认平衡",
        "0.5": "0.5 - 减少重复",
        "1.0": "1.0 - 严格去重"
      }
    },
    {
      "key": "presence_penalty",
      "display": "内容新颖度(presence_penalty)",
      "type": "select",
      "options": {
        "-1.0": "-1.0 - 偏好常见内容",
        "0": "0 - 平衡模式",
        "0.5": "0.5 - 鼓励新内容",
        "1.0": "1.0 - 强创新性"
      }
    },
    {
      "key": "max_tokens",
      "display": "响应长度(max_tokens)",
      "type": "select",
      "options": {
        "500": "500 - 简短响应",
        "1000": "1000 - 中等长度",
        "2000": "2000 - 详细解释",
        "4000": "4000 - 最大长度"
      }
    }
  ],
  "language": {
    "auto": "auto",
    "zh_cn": "zh",
    "zh_tw": "zh_HANT",
    "en": "en",
    "ja": "ja",
    "ko": "ko",
    "fr": "fr",
    "es": "es",
    "ru": "ru",
    "de": "de",
    "it": "it",
    "tr": "tr",
    "pt_pt": "pt",
    "pt_br": "pt",
    "vi": "vi",
    "id": "id",
    "th": "th",
    "ms": "ms",
    "ar": "ar",
    "hi": "hi",
    "mn_cy": "mn",
    "mn_mo": "mn",
    "km": "km",
    "nb_no": "no",
    "nn_no": "no",
    "fa": "fa"
  }
}