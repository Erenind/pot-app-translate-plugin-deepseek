{
  "id": "plugin.com.Erenind.deepseek",
  "plugin_type": "translate",
  "icon": "deepseek.svg",
  "display": "deepseek",
  "homepage": "https://github.com/erenind/pot-app-translate-plugin-deepseek",
  "needs": [
    {
      "key": "model",
      "display": "模型选择",
      "type": "select",
      "options": {
        "deepseek-chat": "deepseek-chat",
        "deepseek-reasoner": "deepseek-reasoner"
      },
      "default": "deepseek-chat"
    },
    {
      "key": "apiKey",
      "display": "API密钥",
      "type": "input"
    },
    {
      "key": "usages",
      "display": "单词信息",
      "type": "select",
      "options": {
        "full": "完整信息(发音/释义/用法)",
        "basic": "基本用法",
        "none": "仅翻译"
      },
      "default": "完整信息(发音/释义/用法)"
    },
    {
      "key": "hyphenMode",
      "display": "连字符短语模式",
      "type": "select",
      "options": {
        "on": "启用(-标识/连接短语,如-make-up)",
        "off": "禁用"
      },
      "default": "启用(-标识/连接短语,如-make-up)"
    },
    {
      "key": "phraseUsages",
      "display": "短语信息",
      "type": "select",
      "options": {
        "full": "详细",
        "basic": "基本解释",
        "none": "仅翻译"
      },
      "default": "详细"
    }
  ],
  "language": {
    "auto": "auto",
    "zh_cn": "zh",
    "zh_tw": "zh_HANT",
    "en": "en",
    "ja": "ja",
    "ko": "ko",
    "fr": "fr",
    "es": "es",
    "ru": "ru",
    "de": "de",
    "it": "it",
    "tr": "tr",
    "pt_pt": "pt",
    "pt_br": "pt",
    "vi": "vi",
    "id": "id",
    "th": "th",
    "ms": "ms",
    "ar": "ar",
    "hi": "hi",
    "mn_cy": "mn",
    "mn_mo": "mn",
    "km": "km",
    "nb_no": "no",
    "nn_no": "no",
    "fa": "fa"
  }
}